---
import Brand from './Brand.astro'
import Nav from './Nav.astro'

const { current = '' } = Astro.props;
---

<style>
  header {
    display: flex;
    margin: 0 auto;
    max-width: 1400px;
    padding: 2em;
    width: 100%;
  }
</style>

<header>
  <Brand />
  <Nav current={current} />
</header>
